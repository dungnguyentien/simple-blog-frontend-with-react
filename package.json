{
	"name": "simple-blog-frontend-with-react",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "nodemon dist/server.js",
		"webpack": "npm-run-all --parallel webpack:server webpack:client",
		"webpack:watch": "npm-run-all --parallel webpack:server:watch webpack:client:watch",
		"webpack:server": "webpack --config src/webpack/webpack.dev.server.config.js",
		"webpack:client": "webpack --config src/webpack/webpack.dev.client.config.js",
		"webpack:server:watch": "webpack --watch --config src/webpack/webpack.dev.server.config.js",
		"webpack:client:watch": "webpack --watch --config src/webpack/webpack.dev.client.config.js",
		"start": "node scripts/start.js",
		"build": "node scripts/build.js",
		"test": "node scripts/test.js"
	},
	"dependencies": {
		"compression": "^1.7.4",
		"express": "^4.17.1",
		"handlebars": "^4.2.0",
		"react": "^16.9.0",
		"react-dom": "^16.9.0"
	},
	"devDependencies": {
		"@babel/core": "^7.5.5",
		"@babel/polyfill": "^7.4.4",
		"@babel/preset-env": "^7.5.5",
		"@babel/preset-react": "^7.0.0",
		"@babel/register": "^7.5.5",
		"babel-loader": "^8.0.6",
		"babel-plugin-react-svg": "^3.0.3",
		"css-loader": "^3.2.0",
		"html-loader": "^0.5.5",
		"isomorphic-style-loader": "^5.1.0",
		"node-sass": "^4.12.0",
		"nodemon": "^1.19.2",
		"npm-run-all": "^4.1.5",
		"postcss-loader": "^3.0.0",
		"sass-loader": "^8.0.0",
		"style-loader": "^1.0.0",
		"svg-inline-loader": "^0.8.0",
		"webpack": "^4.39.3",
		"webpack-cli": "^3.3.8",
		"webpack-node-externals": "^1.7.2",
		"file-loader": "^4.2.0"
	}
}
